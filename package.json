{
  "name": "omni-lordbabuino",
  "version": "0.0.1",
  "description": "omni CLI",
  "bin": {
    "omni": "bin/omni"
  },
  "scripts": {
    "format": "prettier --write **/*.{js,json} && standard --fix",
    "lint": "standard",
    "test": "jest",
    "unit": "jest -c jest.config.unit.js",
    "unit-watch": "jest -c jest.config.unit.js --watch",
    "integration": "jest -c jest.config.integration.js -i",
    "snapupdate": "jest --updateSnapshot",
    "coverage": "jest --coverage"
  },
  "files": [
    "src",
    "LICENSE",
    "readme.md",
    "docs",
    "bin"
  ],
  "license": "MIT",
  "dependencies": {
    "gluegun": "^4.3.1",
    "inquirer": "^7.3.2",
    "ramda": "^0.27.0",
    "sucrase": "^3.15.0"
  },
  "devDependencies": {
    "@sucrase/jest-plugin": "^2.0.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "standard": "^14.3.4"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
